<%= form_with model: @rental, local: true do |f| %>
  <%= f.label :start_date,  'Data de inicio'%>
  <%= f.date_field :star_date %>
  <%= f.label :end_date,  'Data de fim'%>
  <%= f.date_field :end_date %>
  <%= f.label :client_id,  'Cliente'%>
  <%= f.collection_select :client_id , @clients, :id, :descpription %>
  <%= f.label :car_category_id,  'Categoria de Carro'%>
  <%= f.collection_select :car_category_id , @car_category, :id, :name %>
  <%= f.submit, 'Enviar' %>
